# TradingAgents Environment Configuration
# Copy this file to .env and fill in your API keys

# =================================================================
# REQUIRED: OpenRouter API Configuration
# =================================================================

# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# =================================================================
# OPTIONAL: OpenRouter Analytics Tracking
# =================================================================

# Track usage and model performance in OpenRouter dashboard
OPENROUTER_SITE_URL=https://github.com/ee-ortiz/TradingAgents
OPENROUTER_SITE_NAME=TradingAgents

# =================================================================
# OPTIONAL: Custom Directories
# =================================================================

# Custom data directory (defaults to ./data)
TRADINGAGENTS_DATA_DIR=./data

# Custom results directory (defaults to ./results)
TRADINGAGENTS_RESULTS_DIR=./results
